{
  "name": "image-saver",
  "version": "0.0.0",
  "scripts": {
    "kill": "taskkill /F /im node.exe",
    "ng": "ng",
    "ng:serve": "ng serve",
    "start": "npm-run-all -p ng:serve electron:serve",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "build": "npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "rebuilder": "electron-rebuild -f -o sqlite3 -p",
    "postinstall": "npm run postinstall:electron && electron-builder install-app-deps && npm run rebuilder",
    "postinstall:electron": "node postinstall"
  },
  "main": "main.js",
  "private": true,
  "dependencies": {
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.20",
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/cli": "~8.3.20",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~12.12.18",
    "@types/sqlite3": "^3.1.5",
    "codelyzer": "^5.0.1",
    "core-js": "^3.5.0",
    "electron": "^7.1.5",
    "electron-builder": "^22.2.0",
    "electron-rebuild": "^1.8.8",
    "electron-reload": "^1.5.0",
    "hammerjs": "^2.0.8",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.2",
    "rxjs": "~6.5.3",
    "sqlite3": "^4.1.1",
    "ts-node": "~8.5.4",
    "tslint": "~5.20.1",
    "typescript": "~3.5.3",
    "wait-on": "^3.3.0",
    "zone.js": "~0.10.2"
  }
}
