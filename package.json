{
  "name": "image-saver",
  "version": "0.0.0",
  "scripts": {
    "kill": "kill node.exe",
    "ng": "ng",
    "ng:serve": "ng serve",
    "start": "npm-run-all -p ng:serve electron:serve",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "build": "npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "rebuilder": "electron-rebuild -f -o @journeyapps/sqlcipher -p",
    "postinstall": "npm run postinstall:electron && electron-builder install-app-deps && npm run rebuilder",
    "postinstall:electron": "node postinstall"
  },
  "main": "main.js",
  "private": true,
  "dependencies": {
    "@journeyapps/sqlcipher": "^4.1.0",
    "sqlite3": "^4.2.0",
    "tslib": "^2.0.1"
},
"devDependencies": {
	"@angular-devkit/build-angular": "~0.1000.8",
    "@angular/animations": "^10.0.14",
    "@angular/cdk": "^10.1.3",
    "@angular/cli": "~10.0.8",
    "@angular/common": "~10.0.14",
    "@angular/compiler": "~10.0.14",
    "@angular/compiler-cli": "~10.0.14",
    "@angular/core": "~10.0.14",
    "@angular/forms": "~10.0.14",
    "@angular/language-service": "~10.0.14",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "~10.0.14",
    "@angular/platform-browser-dynamic": "~10.0.14",
    "@angular/router": "~10.0.14",
    "@types/jasmine": "~3.5.14",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~14.6.1",
    "@types/sqlite3": "^3.1.5",
    "codelyzer": "^6.0.0",
    "core-js": "^3.6.5",
    "electron": "^11.5.0",
    "electron-builder": "^22.8.0",
    "electron-rebuild": "^2.0.1",
    "electron-reload": "^1.5.0",
    "hammerjs": "^2.0.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
	"ngx-virtual-scroller": "^4.0.3",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "rxjs": "~6.6.2",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "typescript": "~3.9.7",
    "wait-on": "^5.2.0",
    "zone.js": "~0.11.1"
  }
}
