{
  "name": "image-saver",
  "version": "0.0.0",
  "scripts": {
    "kill": "kill node.exe",
    "ng": "ng",
    "ng:serve": "ng serve",
    "start": "npm-run-all -p ng:serve electron:serve",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "build": "npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "rebuilder": "electron-rebuild -f -o @journeyapps/sqlcipher -p",
    "postinstall": "electron-builder install-app-deps && npm run rebuilder"
  },
  "main": "main.js",
  "private": true,
  "dependencies": {
    "@journeyapps/sqlcipher": "^5.3.1",
    "sqlite3": "^5.1.5",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^14.0.1",
    "@angular-devkit/build-angular": "~14.2.1",
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.0",
    "@angular/cli": "~14.2.1",
    "@angular/common": "~14.2.0",
    "@angular/compiler": "~14.2.0",
    "@angular/compiler-cli": "~14.2.0",
    "@angular/core": "~14.2.0",
    "@angular/forms": "~14.2.0",
    "@angular/language-service": "~14.2.0",
    "@angular/material": "^14.2.0",
    "@angular/platform-browser": "~14.2.0",
    "@angular/platform-browser-dynamic": "~14.2.0",
    "@angular/router": "~14.2.0",
    "@electron/remote": "^2.0.8",
    "@types/jasmine": "~4.3.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "~18.7.13",
    "@types/sqlite3": "^3.1.5",
    "codelyzer": "^6.0.2",
    "core-js": "^3.25.0",
    "electron": "^20.1.0",
    "electron-builder": "^23.3.3",
    "electron-rebuild": "^3.2.9",
    "electron-reload": "^2.0.0-alpha.1",
    "hammerjs": "^2.0.8",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ngx-virtual-scroller": "^4.0.3",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "process": "^0.11.10",
    "protractor": "~7.0.0",
    "rxjs": "~7.5.6",
    "ts-node": "~10.9.1",
    "tslint": "~6.1.3",
    "typescript": "~4.8.2",
    "wait-on": "^6.0.1",
    "zone.js": "~0.11.8"
  }
}
